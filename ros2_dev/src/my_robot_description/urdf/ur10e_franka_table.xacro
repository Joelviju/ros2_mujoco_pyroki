<?xml version="1.0"?>
<robot name="leo" xmlns:xacro="http://ros.org/wiki/xacro">

  <link name="world"/>

  <!-- Include Robot Macros -->
  <xacro:include filename="$(find my_robot_description)/urdf/ur10e_macro.xacro"/>
 <xacro:include filename="$(find my_robot_description)/urdf/utils_1.xacro"/>
  <xacro:include filename="$(find my_robot_description)/urdf/franka_hand_1.xacro"/>



    
  <!-- Load Inertial Properties -->
  <xacro:property name="ee_inertials">
    <xacro:map>
      <hand>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass>0.6544</mass>
        <inertia>
          <xx>0.00186</xx>
          <xy>0.0</xy>
          <xz>0.0</xz>
          <yy>0.00030</yy>
          <yz>-0.00002</yz>
          <zz>0.00174</zz>
        </inertia>
      </hand>
      <leftfinger>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass>0.0291</mass>
        <inertia>
          <xx>0.00000849</xx>
          <xy>0.0</xy>
          <xz>0.0</xz>
          <yy>0.00000853</yy>
          <yz>-0.00000106</yz>
          <zz>0.00000177</zz>
        </inertia>
      </leftfinger>
      <rightfinger>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass>0.0291</mass>
        <inertia>
          <xx>0.00000849</xx>
          <xy>0.0</xy>
          <xz>0.0</xz>
          <yy>0.00000853</yy>
          <yz>-0.00000106</yz>
          <zz>0.00000177</zz>
        </inertia>
      </rightfinger>
    </xacro:map>
  </xacro:property>

  <!-- Load Robots -->
  <xacro:ur10e prefix="robot1_"/>
  

  <!-- Attach Franka Hands to UR5 and KUKA -->
  <xacro:franka_hand connected_to="robot1_tool0"
                     arm_id="robot1_arm"
                     ee_id="franka_hand_white"
                     ee_inertials="ee_inertials"
                     description_pkg="franka_description"
                     gazebo="true"/>

 
  <!-- World placement joints -->
  <joint name="robot1_world_to_world" type="fixed">
    <parent link="world"/>
    <child link="robot1_world"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <joint name="robot1_joint_to_world" type="fixed">
    <parent link="robot1_world"/>
    <child link="robot1_base_link"/>
    <origin xyz="0 0 0.75" rpy="0 0 0"/>
  </joint>

  <link name="table">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="1.0 1.0 0.75"/> 
            </geometry>
            <material name="Grey">
                <color rgba="0.7 0.7 0.7 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="1.0 1.0 0.75"/> 
            </geometry>
        </collision>
        <inertial>
            <mass value="500"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>
    <gazebo reference="table">
        <material>Gazebo/Black</material>
        <!-- <transparency>1.0</transparency> -->
    </gazebo>

    <joint name="table_to_world" type="fixed">
        <parent link="world"/>
        <child link="table"/>
        <origin xyz="0.0 0.6 0.375" rpy="0.0 0.0 0.0"/>
    </joint>

  <link name="camera">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.05" />
      </geometry>
      <material name="blue">
        <color rgba="0.0 0.0 1.0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.1 0.1 0.1" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <inertia ixx="0.000166667" ixy="0.0" ixz="0.0" iyy="0.000166667" iyz="0.0" izz="0.000166667"/>
    </inertial>
  </link>
  <joint name="world_to_camera" type="fixed">
    <parent link="world"/>
    <child link="camera"/>
    <origin xyz="0.0 0.6 1.75" rpy="0.0 ${pi/2} 0.0"/>
  </joint>

  <gazebo reference="camera">
    <sensor name="camera" type="camera">
      <camera>
        <camera_info_topic>camera/camera_info</camera_info_topic>
        <horizontal_fov>1.047</horizontal_fov>
        <image>
          <width>320</width>
          <height>240</height>
        </image>
        <clip>
          <near>0.1</near>
          <far>100</far>
        </clip>
      </camera>
      <always_on>1</always_on>
      <update_rate>30</update_rate>
      <visualize>true</visualize>
      <topic>camera/image_raw</topic>
    </sensor> 
    <!--<sensor name="depth_camera1" type="depth_camera">
          <update_rate>10</update_rate>
          <topic>depth_camera</topic>
          <camera>
            <horizontal_fov>1.05</horizontal_fov>
            <image>
              <width>256</width>
              <height>256</height>
              <format>R_FLOAT32</format>
            </image>
            <clip>
              <near>0.1</near>
              <far>10.0</far>
            </clip>
          </camera>
          </sensor> -->
  </gazebo>



  

  <!-- Tables and camera can be included here, omitted for brevity -->
</robot>
